{
  "name": "digital-garden",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "tsc --noEmit && pnpm dlx prettier --check \"**/*.{js,ts,md,mdx,svelte,astro}\"",
    "lint": "prettier --write \"**/*.{js,ts,md,mdx,svelte,astro}\" && eslint --fix \"src/**/*.{js,ts,md,mdx,svelte,astro}\"",
    "commitlint": "commitlint --edit",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "astro": "^3.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@typescript-eslint/parser": "^6.5.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.48.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "husky": "^8.0.0",
    "inquirer": "8",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.12.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
